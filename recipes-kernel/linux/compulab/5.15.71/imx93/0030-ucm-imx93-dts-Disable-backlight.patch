From 0f1f8228b5c3acd80dafb6cecafdecd223f973a3 Mon Sep 17 00:00:00 2001
From: Valentin Raevsky <valentin@compulab.co.il>
Date: Sun, 7 May 2023 13:38:04 +0300
Subject: [PATCH] ucm-imx93: dts: Disable backlight

Signed-off-by: Valentin Raevsky <valentin@compulab.co.il>
---
 arch/arm64/boot/dts/compulab/ucm-imx93-lvds.dtsi | 3 ++-
 arch/arm64/boot/dts/compulab/ucm-imx93-mipi.dtsi | 3 ++-
 2 files changed, 4 insertions(+), 2 deletions(-)

diff --git a/arch/arm64/boot/dts/compulab/ucm-imx93-lvds.dtsi b/arch/arm64/boot/dts/compulab/ucm-imx93-lvds.dtsi
index ed6440bb5b5f..6cbb2c7e785f 100644
--- a/arch/arm64/boot/dts/compulab/ucm-imx93-lvds.dtsi
+++ b/arch/arm64/boot/dts/compulab/ucm-imx93-lvds.dtsi
@@ -35,7 +35,8 @@
 
 	lvds0_panel {
 		compatible = "boe,hv070wsa-100";
-		backlight = <&lvds_backlight>;
+		/*To be enabled as soon as it designed by hw-dep*/
+		/*backlight = <&lvds_backlight>;*/
 		enable-gpios = <&pca9555 6 GPIO_ACTIVE_HIGH>;
 
 		port {
diff --git a/arch/arm64/boot/dts/compulab/ucm-imx93-mipi.dtsi b/arch/arm64/boot/dts/compulab/ucm-imx93-mipi.dtsi
index b9eca0e8d447..dbb3ace29260 100644
--- a/arch/arm64/boot/dts/compulab/ucm-imx93-mipi.dtsi
+++ b/arch/arm64/boot/dts/compulab/ucm-imx93-mipi.dtsi
@@ -48,7 +48,8 @@
 	status = "okay";
 	panel@0 {
 		compatible = "startek,kd050hdfia020";
-		backlight = <&dsi_backlight>;
+		/*To be enabled as soon as it designed by hw-dep*/
+		/*backlight = <&dsi_backlight>;*/
 		reset-gpio = <&pca9555 4 GPIO_ACTIVE_LOW>;
 		reg = <0>;
 		dsi-lanes = <4>;
-- 
2.17.1

