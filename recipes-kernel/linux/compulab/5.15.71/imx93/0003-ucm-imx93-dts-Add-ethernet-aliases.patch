From 864499ee3535710d7494eb0b2dc09f0b0e424e02 Mon Sep 17 00:00:00 2001
From: Valentin Raevsky <valentin@compulab.co.il>
Date: Fri, 7 Apr 2023 12:26:21 +0300
Subject: [PATCH] ucm-imx93: dts: Add ethernet aliases

Signed-off-by: Valentin Raevsky <valentin@compulab.co.il>
---
 arch/arm64/boot/dts/compulab/ucm-imx93.dts | 7 ++++++-
 1 file changed, 6 insertions(+), 1 deletion(-)

diff --git a/arch/arm64/boot/dts/compulab/ucm-imx93.dts b/arch/arm64/boot/dts/compulab/ucm-imx93.dts
index 047c709c82ab..95c11d0ee347 100644
--- a/arch/arm64/boot/dts/compulab/ucm-imx93.dts
+++ b/arch/arm64/boot/dts/compulab/ucm-imx93.dts
@@ -12,6 +12,11 @@
 	model = "CompuLab UCM-i.MX93 board";
 	compatible = "compulab,ucm-imx93", "fsl,imx93";
 
+	aliases {
+		ethernet0 = &eqos;
+		ethernet1 = &fec;
+	};
+
 	chosen {
 		stdout-path = &lpuart1;
 	};
@@ -278,7 +283,7 @@
 	phy-mode = "rgmii-id";
 	phy-handle = <&ethphy2>;
 	fsl,magic-packet;
-	status = "okay";
+	status = "disabled";
 
 	mdio {
 		#address-cells = <1>;
-- 
2.17.1

