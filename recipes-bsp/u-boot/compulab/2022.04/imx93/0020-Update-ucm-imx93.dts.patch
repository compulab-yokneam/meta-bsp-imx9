 	pinctrl-1 = <&pinctrl_lpi2c1>;
 	status = "okay";
-
-        pca9555:pca9555@20 {
-                compatible = "nxp,pca9555";
-                gpio-controller;
-                #gpio-cells = <2>;
-                reg = <0x20>;
-        };
 };
 
